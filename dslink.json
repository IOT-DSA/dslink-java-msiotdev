{
  "name": "dslink-java-msiotdev",
  "version": "0.0.2",
  "description": "DSA over Microsoft IoT",
  "license": "Apache",
  "author": {
    "name": "Kenneth Endfinger",
    "email": "kaendfinger@gmail.com"
  },
  "main": "bin/dslink-java-msiotdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-msiotdev"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-msiotdev/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "DSA-MSIOT"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.msiotdev.IotLinkHandler"
    }
  }
}
